<query id="getProductByDocumentSrl" action="select">
	<tables>
		<table name="products" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
    <conditions>
        <condition operation="equal" column="product_srl" var="product_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
