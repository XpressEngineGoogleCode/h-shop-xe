<query id="getProductImageCount" action="select">
	<tables>
		<table name="product_image" />
	</tables>
	<columns>
		<column name="count(*)" alias="count" />
	</columns>
	<conditions>
        <condition operation="like" column="document_srl" var="document_srl" filter="number" notnull="notnull" />
	</conditions>
</query>