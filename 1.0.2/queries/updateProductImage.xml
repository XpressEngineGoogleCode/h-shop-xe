<query id="updateProductImage" action="update">
    <tables>
        <table name="product_image" />
    </tables>
    <columns>
		<column name="at_index" var="at_index" />
    </columns>
    <conditions>
        <condition operation="equal" column="image_srl" var="image_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
