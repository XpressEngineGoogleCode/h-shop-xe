<query id="updateProduct" action="update"> 
	<tables> 
		<table name="products" /> 
	</tables> 
	<columns> 
		<column name="product_srl" var="product_srl" /> 
		<column name="target_srl" var="document_srl" /> 
		<column name="code" var="code" /> 
		<column name="price_base" var="origin_price" /> 
		<column name="price_stand" var="stand_price" /> 
		<column name="price_sale" var="sale_price" /> 
		<column name="buyed_point" var="give_point" /> 
		<column name="maker" var="maker" /> 
		<column name="brand" var="brand" /> 
		<column name="origin" var="origin" /> 
		<column name="master_image" var="master_image" /> 
		<column name="image_align" var="image_align" /> 
	</columns> 
	<conditions> 
		<condition operation="equal" column="target_srl" var="document_srl" filter="number" notnull="notnull" /> 
	</conditions> 
</query>