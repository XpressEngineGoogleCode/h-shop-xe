<filter name="insert_cart" module="shopxe" act="insertCartCookie">
	<form />
    <parameter>
        <param name="target_var" target="target_var" />
        <param name="volume_var" target="volume_var" />
    </parameter>
    <response callback_func="completeInsertCart">
        <tag name="shopxe_mid" />
        <tag name="insert_type" />
        <tag name="error" />
        <tag name="message" />
    </response>
</filter>
